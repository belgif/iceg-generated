    {
      "EA-Guid": "{68F9C64F-B2BC-4cdc-983F-633E88F5A446}",
      "name": "RegisteredPerson",
      "label": {
        "en": "Registered Person"
      },
      "definition": {
        "en": "Person whose details are officially listed in a register."
      },
      "usage": {
        "en": "This register is usually a population register, but it can also be an electoral register, for example. The registered data relates to the identity (e.g. family and given name) and the place of residence of the person and to important life events such as birth, marriage, decease, etc. These data are typically registered by the government, they offer the registered person legal protection and they allow the government to compile basic statistics about its population."
      }
    },
    {
      "EA-Guid": "{39842E58-770A-45ff-8987-5939EE40B446}",
      "name": "Residency",
      "label": {
        "en": "Residency"
      },
      "definition": {
        "en": "Describes the situation where an individual resides or maintains a residence in a specific jurisdiction, without necessarily implying any legal status or entitlements associated with that presence."
      }
    },
    {
      "EA-Guid": "{ADF14A72-93D4-4305-BAE7-D21FF167C213}",
      "name": "StatutoryCohabitation",
      "label": {
        "en": "Statutory Cohabitation"
      },
      "definition": {
        "en": "Arrangement whereby two people who are not married live together."
      },
      "usage": {
        "en": "Can, just like a marriage, form the basis of a household."
      }
    },
