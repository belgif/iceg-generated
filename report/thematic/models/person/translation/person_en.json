    {
      "EA-Guid": "{E12D29AC-62A7-4b7a-A4B5-948D6FFDB40E}",
      "name": "CivilStatus",
      "label": {
        "en": "Civil Status"
      },
      "definition": {
        "en": "Status of a person according to civil law."
      },
      "usage": {
        "en": "Refers to marriage, civil partnership, descent, guardianship, etc. It is, in other words, the condition of specific relationships between individuals."
      }
    },
    {
      "EA-Guid": "{8762DAE0-F5E0-4016-A694-2CFBB90BF82B}",
      "name": "Death",
      "label": {
        "en": "Death"
      },
      "definition": {
        "en": "Death of a person."
      }
    },
    {
      "EA-Guid": "{1B563855-E1D4-4337-9C32-A823CCB602A4}",
      "name": "Descent",
      "label": {
        "en": "Descent"
      },
      "definition": {
        "en": "Descent is the descending line of blood relationship between different generations."
      },
      "usage": {
        "en": "The descent can be biological as well as non-biological, eg adoption. Descent can also be viewed in the ascending line. The concept is narrower than kinship that also includes marriage. There must be a direct link between the persons (no link between child and grandparent)."
      }
    },
    {
      "EA-Guid": "{F24E6DB6-BEC8-4319-BB3C-600F53305FCF}",
      "name": "DiplomaticStatus",
      "label": {
        "en": "Diplomatic Status"
      },
      "definition": {
        "en": "Legal designation granted to natural persons, including diplomats and personnel of international organizations, who represent their home country or organization in a foreign country."
      }
    },
    {
      "EA-Guid": "{57DDEECB-14B7-46b7-B1D0-CA8494CA55D3}",
      "name": "DomesticResidence",
      "label": {
        "en": "Domestic Residence"
      },
      "definition": {
        "en": "Place within the jurisdiction of the registry where a person lives or stays, either permanently or temporarily."
      }
    },
    {
      "EA-Guid": "{2DDE3B9A-BED5-41f0-89C2-61DFE275B9AD}",
      "name": "EuCitizenship",
      "label": {
        "en": "EU Citizenship"
      },
      "definition": {
        "en": "European Union citizenship is afforded to all nationals of member states of the European Union (EU)."
      }
    },
    {
      "EA-Guid": "{82257739-323D-4e6a-A20C-14710DE402FD}",
      "name": "ForeignResidence",
      "label": {
        "en": "Foreign Residence"
      },
      "definition": {
        "en": "Place outside the jurisdiction of the registry where a person lives or stays, either permanently or temporarily."
      }
    },
    {
      "EA-Guid": "{6EA567A1-A34A-4a9a-B236-9462219AA989}",
      "name": "Guardianship",
      "label": {
        "en": "Guardianship"
      },
      "definition": {
        "en": "Situation where a person acting as guardian is mandated to act on behalf of another person and in that capacity is responsible for this person and his goods."
      },
      "usage": {
        "en": "This is the case, for example, for minors who have lost their parents or for adults who have lost their intellectual faculties."
      }
    },
    {
      "EA-Guid": "{F747041B-2DF2-462f-BA4C-512BB880AE58}",
      "name": "Household",
      "label": {
        "en": "Household"
      },
      "definition": {
        "en": "Form of cooperation that forms a recognizable social unit (eg through marriage), with persons who are blood relatives or not, who have lasting ties and provide mutual support and care."
      },
      "usage": {
        "en": "It is a broader concept than family where the family consists not solely of close relatives, eg a single-person family or living separately together. Household members usually share the same residence."
      }
    },
    {
      "EA-Guid": "{0B5DCF0D-7A27-418d-9356-6C94DF7F404E}",
      "name": "HouseholdRole",
      "label": {
        "en": "Household Role"
      },
      "definition": {
        "en": "Role of the person within the household."
      }
    },
    {
      "EA-Guid": "{257FF210-5084-4fd0-9FAD-C77177BDFB03}",
      "name": "InternationalProtectionStatus",
      "label": {
        "en": "International Protection Status"
      },
      "definition": {
        "en": "Legal designation granted under international law providing specific rights and obligations to natural persons who qualify."
      }
    },
    {
      "EA-Guid": "{DD46B871-D9CC-4c52-81A8-588A7AC8AA61}",
      "name": "Marriage",
      "label": {
        "en": "Marriage"
      },
      "definition": {
        "en": "A form of cohabitation of two persons, organized by civil or religious law."
      },
      "usage": {
        "en": "Can, just like living together, form the basis of a household."
      }
    },
    {
      "EA-Guid": "{BFF63A93-DCFC-468f-B259-203C4B4BE6C0}",
      "name": "PersonRelation",
      "label": {
        "en": "Person Relation"
      },
      "definition": {
        "en": "Relation between two or more natural persons."
      },
      "usage": {
        "en": "Typically, these are civil law relationships (see civil status) but not necessarily limited thereto."
      }
    },
    {
      "EA-Guid": "{B7FBECA6-E45A-4616-9393-70F13C7B5058}",
      "name": "RelationRole",
      "label": {
        "en": "Relation Role"
      },
      "definition": {
        "en": "Role of the person in the relation."
      },
      "usage": {
        "en": "The role is expressed from the source to the target person. For example, Homer (source) has the role father with respect to Lisa (target)."
      }
    },
    {
      "EA-Guid": "{4FEA1F68-630A-41c7-9340-F5B6558B3B46}",
      "name": "PublicLegalStatus",
      "label": {
        "en": "Public Legal Status"
      },
      "definition": {
        "en": "Refers to the legal relationship that exist between a legal subject and a country or official body."
      }
    },
    {
